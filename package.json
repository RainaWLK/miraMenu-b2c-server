  "license": "MIT",
  "private": true,
  "devDependencies": {
    "aws-sdk": "^2.128.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "cross-env": "^5.0.0",
    "serverless": "^1.23.0",
    "serverless-offline": "^3.16.0",
    "serverless-plugin-include-dependencies": "^3.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "lodash": "^4.17.4"
  }
}
